{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for the HTTP Visualizer app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "core:app:allow-app-show",
      "allow": []
    },
    {
      "identifier": "core:window:allow-close",
      "allow": []
    },
    {
      "identifier": "core:window:allow-minimize",
      "allow": []
    },
    {
      "identifier": "core:window:allow-maximize",
      "allow": []
    }
  ]
}
